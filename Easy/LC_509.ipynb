        "**Fibonacci Number**"
      ],
      "metadata": {
        "id": "81YrCcDzvh1q"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def fib(n):\n",
        "  f0 = 0\n",
        "  f1 = 1\n",
        "  if n==0:\n",
        "    return 0\n",
        "  if n == 1:\n",
        "    return 1\n",
        "  nth = f1\n",
        "  while(n>1):\n",
        "    nth = f0+f1\n",
        "    f0,f1 = f1,nth\n",
        "    n-=1\n",
        "  return nth\n",
        "\n",
        "print(fib(4))"
      ],
